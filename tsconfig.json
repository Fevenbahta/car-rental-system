{  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/pages/login/login.jsx",
    "app/Auth/forgotpassword/forgotpassword.jsx",
    "app/Componeapp/otpverification/otpverification.jsx",
    "app/Componeapp/register/register.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}